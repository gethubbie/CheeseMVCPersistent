@Model  CheeseMVC.ViewModels.ViewMenViewModel
@{
    ViewBag.Title = "MENU NAME";
}

<title>@Model.Menu.ID</title>

<h2>Model.Menu.Name</h2>

<ul>
    @foreach (var CheeseMenu in Model.Items)
    {
        <li>
            @CheeseMenu.Cheese.Name
        </li>
    }
</ul>
    <a asp-controller="Menu" asp-action="Add Item" asp-route-id="@Model.Menu.ID">Add Item</a>





    